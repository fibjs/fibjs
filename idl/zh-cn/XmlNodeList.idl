
/*! @brief XmlNodeList 对象代表一个有顺序的节点列表
*/
interface XmlNodeList : object
{
    /*! @brief 返回节点列表中的节点数目
    */
    readonly Integer length;

    /*! @brief 返回节点列表中处于指定的索引号的节点
     @param index 指定要查询的索引
     @return 指定索引号的节点
    */
    XmlNode item(Integer index);

    /*! @brief 可使用下标直接访问数据
    */
    readonly XmlNode operator[];
};