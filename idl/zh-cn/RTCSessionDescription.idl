/*! @brief WebRTC 会话描述对象 */
interface RTCSessionDescription : object
{
    /*! @brief 构造函数

     description 初始化参数，支持以下字段：
        - type: 描述类型
        - sdp: 描述字符串

      @param description 初始化参数
    */
    RTCSessionDescription(Object description = {});

    /*! @brief 返回描述类型 */
    readonly String type;

    /*! @brief 返回描述字符串 */
    readonly String sdp;
};
